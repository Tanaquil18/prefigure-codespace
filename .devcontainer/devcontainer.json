{
    "name": "PreFigure-Codespaces",
    "dockerFile": "Dockerfile",
    "postCreateCommand": "pip install -i https://test.pypi.org/simple/ --extra-index-url https://pypi.org/simple prefig",

    "customizations": {
        "codespaces": {
            "openFiles": ["samples/tangent.xml"]
        },
        "vscode": {
            "extensions": [
                "vitaliymaz.vscode-svg-previewer"
            ]
        }
    }
}
